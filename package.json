{
  "name": "vue-geo-suggest",
  "version": "0.0.0",
  "description": "Renderless Vue component for finding addresses using Google Places API",
  "author": "Fran Dios <me@frandiox.com>",
  "license": "MIT",
  "main": "dist/geo-suggest.umd.js",
  "module": "dist/geo-suggest.common.min.js",
  "scripts": {
    "dev": "vue-cli-service serve ./example/main.js --open --port 8030",
    "build": "vue-cli-service build --name geoSuggest --entry ./src/index.js --target lib",
    "lint": "vue-cli-service lint",
    "doc:build": "vuedoc.md --section API --output ./README.md ./src/geo-suggest.js",
    "prepublishOnly": "npm run lint && npm run build && npm run doc:build",
    "test:unit": "vue-cli-service test:unit"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.5.x",
    "@vue/cli-plugin-eslint": "3.5.x",
    "@vue/cli-plugin-unit-jest": "3.5.x",
    "@vue/cli-service": "3.5.x",
    "@vue/eslint-config-prettier": "4.0.x",
    "@vue/test-utils": "1.0.0-beta.29",
    "@vuedoc/md": "1.6.x",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.x",
    "babel-jest": "24.5.x",
    "eslint": "5.15.x",
    "eslint-plugin-vue": "5.2.x",
    "lint-staged": "8.1.x",
    "lodash.debounce": "4.0.x",
    "material-design-icons-iconfont": "^4.0.5",
    "vue": "2.6.x",
    "vue-cli-plugin-component": "1.10.x",
    "vue-template-compiler": "2.6.x",
    "vuetify": "1.5.x"
  },
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "vue",
    "component",
    "gmaps",
    "maps",
    "google",
    "places",
    "geosuggest",
    "suggest",
    "vuetify"
  ],
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
